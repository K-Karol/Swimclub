<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Swimclub.Mobile.Views.GradingPages" >
    <ContentPage.Content>
        <StackLayout>
            <Label x:Name="StudentName" Text="{Binding ViewStudentName}" Margin="20" FontSize="Large"/>
            <Button x:Name="NewTest" Text="Take a new test" Clicked="NewTest_Clicked" Margin="20"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>